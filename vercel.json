{
  "version": 2,
  "public": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/web/$1"
    }
  ],
  "headers": [
    {
      "source": "/web/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ]
}