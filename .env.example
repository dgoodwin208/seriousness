# Seriousness Evaluator V2 Environment Variables
# Copy this file to .env and fill in your actual credentials

# Anthropic API (for Claude models)
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# OpenAI API (for GPT models)
OPENAI_API_KEY=your-openai-api-key-here

# Supabase (optional - only needed if using database features)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key-here
SUPABASE_PASSWORD=your-database-password-here

# Note: Never commit the .env file to git!
# The .env file is in .gitignore for security
